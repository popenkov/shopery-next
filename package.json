{
  "name": "shopery-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "open-browser-windows": "open http://0.0.0.0:5555",
    "dev-open-browser-windows": "next dev -p 5555 & npm run open-browser-windows",
    "open-browser-linux": "xdg-open http://0.0.0.0:5555",
    "dev-open-browser-linux": "next dev -p 5555 & npm run open-browser-linux",
    "build": "next build",
    "start": "next start",
    "start:dev": "concurrently \"npm run start:dev:server\" \"next dev\"",
    "start:dev:server": "node ./json-server/index.js",
    "lint:ts": "next lint",
    "lint:ts:fix": "eslint -c ./.eslintrc.json \"./**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "prettier": "npx prettier --write **/*.{ts,tsx,json}",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "test:unit": "jest --config ./src/app/configs/jest/jest.config.ts",
    "test:ui": "test-storybook --config-dir ./src/app/configs/storybook",
    "test:ui:ci": "concurrently -s first -k \"npm run storybook\" \"npm run test:ui\"",
    "test:e2e": "npx cypress open",
    "test:e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "prepare": "husky",
    "depcruise": "npx depcruise src --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph.svg",
    "depcruise:win": "npx depcruise src --include-only \"^src\" --output-type dot | cmd /c path C:/Program Files/Graphviz/bin/dot.exe -T svg > dependency-graph.svg",
    "storybook": "storybook dev -p 6006 -c ./src/app/configs/storybook --debug-webpack",
    "build-storybook": "storybook build -c ./src/app/configs/storybook",
    "generate-icons-with-export": "node ./scripts/generate-icons-with-export.js"
  },
  "dependencies": {
    "@types/circular-dependency-plugin": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "circular-dependency-plugin": "^5.2.2",
    "classnames": "2.5.1",
    "concurrently": "^8.2.2",
    "eslint-plugin-unused-imports": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "next": "14.1.0",
    "next-plugin-svgr": "1.1.10",
    "rc-slider": "10.5.0",
    "react": "18",
    "react-dom": "18",
    "react-select": "5.8.0",
    "regenerator-runtime": "^0.14.1",
    "sass": "1.70.0",
    "swiper": "11.0.7",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/plugin-transform-typescript": "7.18.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.24.1",
    "@chromatic-com/storybook": "^1.3.2",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@feature-sliced/eslint-plugin-messages": "^0.1.0-beta.2",
    "@next/eslint-plugin-next": "14.1.0",
    "@storybook/addon-a11y": "^8.0.8",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-onboarding": "^8.0.8",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^8.0.8",
    "@storybook/nextjs": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@storybook/test-runner": "^0.17.0",
    "@svgr/webpack": "8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "babel-jest": "^29.7.0",
    "cypress": "^13.8.0",
    "dependency-cruiser": "^16.3.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.2.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-storybook": "0.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "lint-staged": "^15.2.2",
    "msw": "^1.3.3",
    "msw-storybook-addon": "^1.10.0",
    "nanoid": "^5.0.6",
    "prettier": "^3.2.5",
    "react-hook-form": "^7.51.4",
    "react-imask": "^7.6.0",
    "storybook": "^8.0.8",
    "stylelint": "16.3.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.1.0",
    "ts-jest": "^29.1.2",
    "typescript": "5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "Chrome 118",
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different",
      "next lint"
    ],
    "*.scss": "lint:scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
